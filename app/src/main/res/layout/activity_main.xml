<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="25sp"
    android:background="#343434"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/summary"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="410sp"
        android:layout_height="225sp"
        android:background="#4A4A4A"
        android:clickable="false"
        android:contextClickable="false"
        android:cursorVisible="false"
        android:editable="false"
        android:ems="10"
        android:gravity="bottom|end"
        android:linksClickable="false"
        android:longClickable="false"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="50sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_0"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="0"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_1"
        app:layout_constraintTop_toBottomOf="@+id/button_Equals"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_1"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="1"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button_0"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_2"
        app:layout_constraintTop_toBottomOf="@+id/button_4"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_2"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="2"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button_1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_3"
        app:layout_constraintTop_toBottomOf="@+id/button_5"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_3"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="3"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button_2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_6"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_4"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="4"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_1"
        app:layout_constraintEnd_toStartOf="@+id/button_Equals"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_5"
        app:layout_constraintTop_toBottomOf="@+id/button_7"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_5"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="5"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_2"
        app:layout_constraintEnd_toStartOf="@+id/button_4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_6"
        app:layout_constraintTop_toBottomOf="@+id/button_8"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_6"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="6"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_3"
        app:layout_constraintEnd_toStartOf="@+id/button_5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_9"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_7"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="7"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_4"
        app:layout_constraintEnd_toStartOf="@+id/button_Minus"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_8"
        app:layout_constraintTop_toBottomOf="@+id/button_Plus"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_8"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="8"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_5"
        app:layout_constraintEnd_toStartOf="@+id/button_7"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_9"
        app:layout_constraintTop_toBottomOf="@+id/button_Mult"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_9"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="9"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_6"
        app:layout_constraintEnd_toStartOf="@+id/button_8"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_Dot"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_Mult"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="X"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_8"
        app:layout_constraintEnd_toStartOf="@+id/button_Plus"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_Dot"
        app:layout_constraintTop_toBottomOf="@+id/button_Percent"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_Plus"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="+"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_7"
        app:layout_constraintEnd_toStartOf="@+id/button_Back"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_Mult"
        app:layout_constraintTop_toBottomOf="@+id/button_Parentheses"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_Equals"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="#F9F9F9"
        android:text="="
        android:textColor="#E8CD10"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_4"
        app:layout_constraintTop_toBottomOf="@+id/button_Minus"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_Parentheses"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="( )"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_Plus"
        app:layout_constraintEnd_toStartOf="@+id/button_Del"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_Percent"
        app:layout_constraintTop_toBottomOf="@+id/summary"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_Div"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="รท"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_Dot"
        app:layout_constraintEnd_toStartOf="@+id/button_Percent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/summary"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_Minus"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="-"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_Equals"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_7"
        app:layout_constraintTop_toBottomOf="@+id/button_Back"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_Back"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="Back"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_Minus"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_Plus"
        app:layout_constraintTop_toBottomOf="@+id/button_Del"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_Del"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="#524F4F"
        android:text="CE"
        android:textColor="#9C0404"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_Back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_Parentheses"
        app:layout_constraintTop_toBottomOf="@+id/summary"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_Dot"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="."
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_9"
        app:layout_constraintEnd_toStartOf="@+id/button_Mult"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_Div"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button_Percent"
        android:layout_width="90sp"
        android:layout_height="100sp"
        android:backgroundTint="@color/button_gray"
        android:text="%"
        android:textSize="30sp"
        app:cornerRadius="35sp"
        app:layout_constraintBottom_toTopOf="@+id/button_Mult"
        app:layout_constraintEnd_toStartOf="@+id/button_Parentheses"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_Div"
        app:layout_constraintTop_toBottomOf="@+id/summary"
        app:layout_constraintVertical_bias="0.5" />

</androidx.constraintlayout.widget.ConstraintLayout>